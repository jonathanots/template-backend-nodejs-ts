{
    "env": {
        // "browser": true,
        // "es2021": true,
        "es6": true
    },
    "extends": [
        // "standard",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "no-useless-constructor": "off",
        "semi" : "off",
        "indent" : "off"
    }
    
}
